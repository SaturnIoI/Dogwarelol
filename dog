}

end

local Settings = {
Prediction_Settings = {
    AutoSettings = false,
    Prediction = 0.165
},
Aimbot = {
    Enabled = false,
    Aiming = false,
    FOV = {
        Enabled = false,
        Size = 100,
        Round = 100,
        Color = Color3.fromRGB(0, 145, 255),
        Shape = "Custom",
        Filled = false,
        Transparency = 1
    },
    Hitbox = "Head",
    Nearest = "Mouse",
    VisibleCheck = false,
    IgnoreFOV = false,
},
SilentAim = {
    Enabled = false,
    WallCheck = false,
    FOV = {
        Enabled = false,
        Size = 100,
        Round = 100,
        Color = Color3.fromRGB(28, 56, 139),
        Shape = "Custom",
        Filled = false,
        Transparency = 0.5
    },
    Hitbox = "HumanoidRootPart",
    Nearest = "Mouse",
    Mode = "Legit",
    VisibleCheck = false,
    IgnoreFOV = false,
    LookAt = false,
},
